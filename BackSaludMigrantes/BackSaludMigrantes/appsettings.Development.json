{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Connection":"Server={{DB_ENDPOINT}},{{DB_PORT}};Database={{DB_NAME}};ConnectRetryCount=0;User Id={{DB_USER}};Password={{DB_PASSWORD}};MultipleActiveResultSets=true"
  }
}
